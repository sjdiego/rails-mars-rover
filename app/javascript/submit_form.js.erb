!(() => {
  let form = document.querySelector("#input_form");
  form.addEventListener("submit", (event, data) => {
    event.preventDefault();
    Rails.ajax({
      url: '/',
      type: 'POST',
      data: form.data,
      success: (response) => {
        console.log(response)
      },
      error: (err) => {
        console.log(err)
      }
    })
  });
})()